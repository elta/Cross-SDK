+	prev_heap_end  = heap_end;
+	heap_end      += incr;
+	//check
+	if( heap_end < (&__heap_end)) {
+
+	} else {
+		errno = ENOMEM;
+		return (char*)-1;
+	}
+	return (void *) prev_heap_end;
+
+}	/* _sbrk () */
