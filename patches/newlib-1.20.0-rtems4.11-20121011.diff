+	.file "setjmp.S"
+        .section .text
+        .align 4
+	.global  _setjmp
+        .type _setjmp, %function
+_setjmp:
+	strd lr,[r0]
+	strd r4,[r0,1]
+	strd r6,[r0,2]
+	strd r8,[r0,3]
+	strd r10,[r0,4]
+	strd r32,[r0,5]
+	strd r34,[r0,6]
+	strd r36,[r0,7]
+	strd r38,[r0,8]
+	str sp,[r0,18]
+	mov r0,#0
+	rts
+	.size _setjmp, .-_setjmp
+
+	.global _longjmp
+_longjmp:
+	ldrd lr,[r0] ; return address / r15
+	ldrd r4,[r0,1]
+	ldrd r6,[r0,2]
+	ldrd r8,[r0,3]
+	ldrd r10,[r0,4]
+	ldrd r32,[r0,5]
+	ldrd r34,[r0,6]
+	ldrd r36,[r0,7]
+	ldrd r38,[r0,8]
+	ldr sp,[r0,18]
+	sub r1,r1,0
+	mov r0,#1
+	movne r0,r1
+	jr lr
+        .size   _longjmp, .-_longjmp
