+int _kill _PARAMS ((int, int));
+
+int
+_kill (int pid, int sig)
+{
+  param_block_t block[2];
+
+  if (pid == 0)
+    return 0;
+  if (pid != 1 && pid != -1)
+    {
+      errno = ESRCH;
+      return -1;
+    }
+
+  block[0] = ADP_Stopped_RunTimeError;
+  block[1] = sig;
+  return do_AngelSVC (AngelSVC_Reason_ReportException, block);
+}
